	"version": "0.2.0",
	"configurations": [
		{
			"type": "lldb",
			"request": "launch",
			"name": "3 + 1",
			"program": "${workspaceFolder}/push_swap",
			"args": ["3","+","1"],
			"cwd": "${workspaceFolder}"
		},
	  {
		"name": "(gdb) ./push_swap args",
		"type": "cppdbg",
		"request": "launch",
		"program": "${workspaceFolder}/push_swap",
		"args": [
			"${input:args}"
		],
		"stopAtEntry": false,
		"cwd": "${workspaceFolder}",
		"environment": [],
		"externalConsole": false,
		"MIMode": "gdb",
		"preLaunchTask": "make re",
		"setupCommands": [
		  {
			"description": "Enable pretty-printing for gdb",
			"text": "-enable-pretty-printing",
			"ignoreFailures": true
		  }
		]
	  },
	  {
		"type": "lldb",
		"request": "launch",
		"name": "(lldb) ./push_swap args",
		"program": "${workspaceFolder}/push_swap",
		"args": ["${input:args}"],
		"cwd": "${workspaceFolder}",
		"preLaunchTask": "make re"
	},
	],
	"inputs": [
		{
		  "id": "args",
		  "type":"promptString",
		  "description": "args",
